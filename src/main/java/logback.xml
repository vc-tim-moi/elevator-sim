<configuration>
	<appender name="FILE" class="ch.qos.logback.core.FileAppender">
		<file>file.log</file>
		<append>true</append>
		<encoder>
			<pattern>%d{HH:mm:ss.SSS} [%thread] %-5level - %msg%n</pattern>
		</encoder>
	</appender>
	<root level="debug">
		<appender-ref ref="FILE"/>
	</root>
</configuration>